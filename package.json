  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-add-module-exports": "1.0.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-minify": "0.5.1",
    "chai": "4.2.0",
    "cross-env": "7.0.2",
    "eslint": "7.4.0",
